    "scripts": {
        "// BUILD -----------------": "",
        "// ENV -----------------": "",
        "// RUN -----------------": "",
        "// DEV -----------------": "",
        "build": "turbo run build",
        "// Databse -----------------": "",
        "env": "dotenv --",
        "build:env": "npm run env -- turbo run build",
        "start:app": "turbo run preview --prefix=apps/xray-app",
