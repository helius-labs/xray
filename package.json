        "db:generate": "dotenv -- turbo run db:generate",
        "db:pull": "dotenv -- turbo run db:pull",
        "dev": "dotenv -- turbo run dev --parallel",
        "format": "prettier --write \"**/*.{ts,tsx,md,svelte}\"",
        "lint": "turbo lint",
        "lint:fix": "eslint . --fix"
    },
    "version": "0.0.1",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
