{
  "name": "@helius/xray",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "// ENV -----------------": "",
    "env": "dotenv --",
    "// RUN -----------------": "",
    "start:app": "turbo run preview --prefix=apps/xray-app",
    "// BUILD -----------------": "",
    "build": "turbo run build",
    "build:env": "npm run env -- turbo run build",
    "// DEV -----------------": "",
    "dev": "dotenv -- turbo run dev --parallel",
    "// Databse -----------------": "",
    "db:pull": "dotenv -- turbo run db:pull",
    "db:generate": "dotenv -- turbo run db:generate",
    "// Lint --------------------": "",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "*",
    "nodemon": "^2.0.20",
    "prettier": "latest",
    "tsc-watch": "^6.0.0",
    "turbo": "^1.7.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@8.19.2",
  "dependencies": {
    "@solana/web3.js": "^1.73.2",
    "chartist": "^1.3.0",
    "helius-sdk": "^0.3.0",
    "swiper": "^8.4.6"
  }
}
